{
  "name": "Democues",
  "version": "0.1.44",
  "description": "Demo all the parts of Appcues, anywhere.",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
    "permissions": ["activeTab", "declarativeContent", "storage", "webRequestBlocking",  "webRequest", "http://democues.reload/"],
  "page_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "run_at": "document_idle",
      "all_frames": false,
      "css": [],
      "js": ["build/javascriptSDK.js", "contentscript.js"]
    }
  ],
  "manifest_version": 2
}
